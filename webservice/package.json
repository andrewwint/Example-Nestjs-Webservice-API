{
  "name": "@truechoice/webservice",
  "version": "2.1.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "private": true,
  "homepage": "https://bitbucket.org/truechoicesolutions/portal-webservice/webservice#readme",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/truechoicesolutions/portal-webservice.git",
    "directory": "webservice"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "forever start dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:ci": "jest --config ./test/jest-e2e-ci.json",
    "docker:compose:up": "docker-compose -p webservice_local up --build -V -d --no-color",
    "docker:compose:down": "docker-compose -p webservice_local down",
    "docker:webservice:run": "sh docker-run-local.sh",
    "docker:webservice:push": "sh docker-push-ecr.sh"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.5",
    "@nestjs/core": "^7.3.2",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/swagger": "^4.7.9",
    "aws-sdk": "^2.612.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mongoose": "^5.8.0",
    "nestjs-easyconfig": "^1.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pusher": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "uuid": "^3.3.3",
    "xml-js": "^1.6.11",
    "xmlbuilder": "^13.0.2",
    "xmldom": "^0.2.1",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.3",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.10.14",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.1",
    "@types/html-entities": "^1.2.16",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.5.34",
    "@types/node": "^12.7.5",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.0.0",
    "@types/xml2js": "^0.4.5",
    "@types/xmldom": "^0.1.29",
    "jest": "^24.9.0",
    "mongodb-memory-server": "^6.9.2",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "setupFilesAfterEnv": [
      "./test/jest/setTimeout.ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@truechoice/auth/(.*)": "<rootDir>/libs/auth/src/$1",
      "@truechoice/auth": "<rootDir>/libs/auth/src",
      "@truechoice/notification/(.*)": "<rootDir>/libs/notification/src/$1",
      "@truechoice/notification": "<rootDir>/libs/notification/src",
      "@truechoice/users/(.*)": "<rootDir>/libs/users/src/$1",
      "@truechoice/users": "<rootDir>/libs/users/src",
      "@truechoice/ssm/(.*)": "<rootDir>/libs/ssm/src/$1",
      "@truechoice/ssm": "<rootDir>/libs/ssm/src",
      "@truechoice/stacks/(.*)": "<rootDir>/libs/stacks/src/$1",
      "@truechoice/stacks": "<rootDir>/libs/stacks/src",
      "@truechoice/aws/(.*)": "<rootDir>/libs/aws/src/$1",
      "@truechoice/aws": "<rootDir>/libs/aws/src",
      "@truechoice/xml-loader/(.*)": "<rootDir>/libs/xml-loader/src/$1",
      "@truechoice/xml-loader": "<rootDir>/libs/xml-loader/src",
      "@truechoice/helpers/(.*)": "<rootDir>/libs/helpers/src/$1",
      "@truechoice/helpers": "<rootDir>/libs/helpers/src",
      "@truechoice/installer/(.*)": "<rootDir>/libs/installer/src/$1",
      "@truechoice/installer": "<rootDir>/libs/installer/src",
      "@truechoice/status/(.*)": "<rootDir>/libs/status/src/$1",
      "@truechoice/status": "<rootDir>/libs/status/src"
    }
  }
}
